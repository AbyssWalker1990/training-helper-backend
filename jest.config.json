{
    "automock": false,
    "collectCoverage": true,
    "modulePathIgnorePatterns": [
        "<rootDir>/src/",
        "<rootDir>/dist/config",
        "<rootDir>/dist/interfaces",
        "<rootDir>/dist/server.js"
    ],
    "collectCoverageFrom": [
        "dist/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
        "global": {
            "statements": 50,
            "branches": 50,
            "functions": 50,
            "lines": 50
        }
    }
}